{
  "name": "reducerx",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --color --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "echo \"test script\""
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "chrome >= 60",
              "firefox >= 55"
            ]
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "preact": "^8.2.5",
    "rxjs": "^5.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "style-loader": "^0.14.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2"
  }
}
